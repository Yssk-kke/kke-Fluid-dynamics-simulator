FoamFile
{
    version     2.0;
    format      ascii;
    class       IOobject;
}
    
building                                    // building or ground surface
{
    type    opaqueDiffusive;
    wallAbsorptionEmissionModel
    {
        type            multiBandAbsorption;
        absorptivity    (0.30 0.70);     // (Fix) (UserInput)
        emissivity      (0.30 0.70);     // (Fix) (UserInput)
    };
}
    
